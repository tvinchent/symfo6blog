{% extends 'base.html.twig' %}

{% block title %} {{ title }} {% endblock %}

{% block body %}
<h1>THE blog</h1>

{% for post in posts %}

<h2>{{ post.title }}</h2>
<p>{{ post.content }}</p>
<strong>{{ post.author }}</strong><br>
<a href="post/{{ post.id }}">Lien vers le billet</a>

{% endfor %}

{% endblock %}